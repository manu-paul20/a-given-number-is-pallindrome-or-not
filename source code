#include <stdio.h>
int main()
{
    int n, rev = 0, number, rem;
    printf("enter a number:");
    scanf("%d", &n);
    number = n;
    while (n != 0)
    {
        rem = n % 10;
        rev = rev * 10 + rem;
        n = n / 10;
    }
    if (n == rev)
    {
        printf("%d is a pallinedrome", number);
    }
    else
    {
        printf("%d is not a pallindrome", number);
        
    }

    return 0;
}
